{
  "name": "tafu-ui-design",
  "version": "0.1.1",
  "description": "Tafu Design System - React Component Library with full responsive design",
  "type": "module",
  "main": "./dist/tafu-ui-design.cjs",
  "module": "./dist/tafu-ui-design.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tafu-ui-design.mjs",
      "require": "./dist/tafu-ui-design.cjs"
    },
    "./styles": {
      "default": "./dist/tafu-ui-design.css"
    },
    "./styles.css": "./dist/tafu-ui-design.css",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css",
    "**/*.css",
    "./dist/tafu-ui-design.css"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "vite build",
    "build:storybook": "storybook build",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "framer-motion": "^11.0.0",
    "@phosphor-icons/react": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "framer-motion": "^11.15.0",
    "@phosphor-icons/react": "^2.1.7",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "storybook": "^8.6.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.3.0"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "tafu",
    "prismo",
    "typescript",
    "responsive"
  ],
  "author": "Prismo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/prismo-ui"
  },
  "publishConfig": {
    "access": "public"
  }
}
